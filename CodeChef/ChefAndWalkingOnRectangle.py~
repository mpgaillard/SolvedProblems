T = input()

for i in range(T):
    (N,M,K) = raw_input().split()
    N = int(N); M=int(M); K = int(K);
    total_cells = (N*M)-2
    paths = [[0 for x in range(M)] for _ in range(N)]
    print paths
    
    for i in range(N):
        paths[i][0]=1
    for j in range(M):
        paths[0][j]=1
    for i in range(1, N):
        for j in range(1, M):
            paths[i][j] = paths[i-1][j]+paths[i][j-1]

    print paths
